@model nettButikkpls.Models.Cart
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>addOrder</title>

</head>
<body>
   
    <div class="container">
        
        @for (int i = 0; i < Model.count.Count; i++)

        {
            <p>@Model.products[i].productname</p><p>@Model.count[i]</p><p>@Model.price[i]</p>

        }
        <form class="form-horizontal" action="/Orders/NullCart" method="post">

            <div class="form-group">
                <div class="col-sm-10">
                    <input class="btn btn-lg btn-primary btn-block" type="submit" value="Back to the store" />
                </div>
            </div>
        </form>
</div>
</body>
</html>
