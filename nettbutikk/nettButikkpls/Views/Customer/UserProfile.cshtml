@{nettButikkpls.Models.Customers customer = (nettButikkpls.Models.Customers)HttpContext.Current.Session["CurrentUser"]; }


<ul class="list-group fifty">

    <li class="list-group-item active">
        <h2>User Profile</h2>
    </li>

    <li class="list-group-item">
        Mail address: @customer.Mail 
    </li>
    <li class="list-group-item">
        First name: @customer.Firstname
    </li>
    <li class="list-group-item">
        Last name: @customer.Lastname
    </li>
    <li class="list-group-item">
        Address: @customer.Address
    </li>

</ul>
<div>
    <a href="/Customer/Edit">Update user information</a>
</div>